        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_FHFJPZglEee8K6JHEn4neQ" name="bounce" specification="_FF75sZglEee8K6JHEn4neQ">
          <language>java</language>
          <body>if (location.x &lt;= 0 || location.x + 2 * radius >= BallModel.returnBounds().getWidth()) {
			velocity.x = -velocity.x;
			location.x = location.x + 2 * velocity.x;
		}
		if (location.y &lt;= 0 || location.y + 2 * radius >= BallModel.returnBounds().getHeight()) {
			velocity.y = -velocity.y;
			location.y = location.y + 2 * velocity.y;
		}
	</body>
        </ownedBehavior>
