package model;

/**
 * Interface that goes from the model to the view that enables the model to talk to the view
 * @author guoqz
 */
public interface IModel2ViewAdapterCtrl {
	/**
	 * The method that tells the view to update
	 */
	public void update();

	/**
	 * No-op "null" adapter
	 */
	public static final IModel2ViewAdapterCtrl NULL_OBJECT = new IModel2ViewAdapterCtrl() {
		public void update() {

		}
	};
}
