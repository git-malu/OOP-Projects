package lm44_xw47.model;

import common.IUserCmd2ModelAdapter;

/**
 * Following defines the adapter main model uses to communicate with view.
 * 
 * @author Lu Ma
 * @author Xiajun Wu
 */
public interface IMainModel2ViewAdapter {
	/**
	 * Append a string to log area.
	 * 
	 * @param str The string being added to log area.
	 */
	public void appendLog(String str);
	
	/**
	 * Create a IUserCmd2ModelAdapter.
	 * 
	 * @return A IUserCmd2ModelAdpater.
	 */
	public IUserCmd2ModelAdapter createUserCmd2ModelAdapter();
}
