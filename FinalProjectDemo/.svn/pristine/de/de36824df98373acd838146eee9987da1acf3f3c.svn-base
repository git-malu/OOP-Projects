package lm44_xw47.view;

/**
 * The adapter map GUI used to communicate with model.
 * 
 * @author Lu Ma
 * @author Xiaojun Wu
 *
 * @param <CBType> Type.
 */
public interface IMapView2MapModelAdapter<CBType> {
	/**
	 * Go to a specified place based on given latitude and longitude
	 * 
	 * @param latitude The latitude of the place.
	 * @param longitude The longitude of the place.
	 */
	public void goLatLong(String latitude, String longitude);

	/**
	 * Go to the given place.
	 * 
	 * @param o The place will go to.
	 */
	public void goPlace(CBType o);
}
