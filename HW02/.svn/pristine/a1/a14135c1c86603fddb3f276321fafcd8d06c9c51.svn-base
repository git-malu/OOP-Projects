package ballModel;

import java.awt.Color;
import java.awt.Graphics;

public class Ball extends AShape{

	@Override
	public void paint(Graphics g) {
		// TODO Auto-generated method stub
		g.setColor(Color.RED); // Set the color to use when drawing
		g.fillOval(100, 150, 20, 40); // paint a filled 20x40 red ellipse whose upper left corner is at (75, 100)
	}
}

class RadiusBall extends Ball{
	int radius = 10;
	
	public void paint(Graphics g) {
		// TODO Auto-generated method stub
		g.setColor(Color.RED); // Set the color to use when drawing
		g.fillOval(167, 189, radius, radius);
	}
}
