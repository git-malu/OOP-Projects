package shape;

import java.awt.Color;
import java.awt.Graphics;


public class ARect extends AShape{
	public ARect(){
		color = Color.BLUE;
		size = new int[] {23, 44};
		location = new int[] {50, 50};
	}
	
	public void paint(Graphics g) {
		g.setColor(color);
		g.fillRect(location[0], location[1], size[0], size[1]);
	}
}